{
    "name": "tuananhnva/laravel-repository",
    "description": "Laravel Repository Pattern",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anh AiT",
            "email": "tuananhnva@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "AnhAiT\\LaravelRepository\\":"src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnhAiT\\LaravelRepository\\RepositoryServiceProvider"
            ],
            "aliases": {
                "LaravelRepository": "AnhAiT\\LaravelRepository\\Facade"
            }
        }
    }
}
